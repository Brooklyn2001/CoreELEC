URL="$@"
CONFIG_FILE="/storage/.config/acestream/stream-tvh.conf"

LOG="quiet"
ANALYZE=5000000
PROBE=5000000

[ -f $CONFIG_FILE ] && . $CONFIG_FILE

/usr/bin/ffmpeg -v $LOG -analyzeduration $ANALYZE -probesize $PROBE -i "$URL" -c copy -map 0 -f mpegts pipe:1
